---
import Header from "./Header.astro";
import "../styles/global.css";
console.log(Astro.url.pathname);
const defaultDescription: string = "Are you hungry?";

type LayoutProps = {
  lang?: string;
  pageTitle?: string;
  description?: string;
};

const props: LayoutProps = Astro.props;
---

